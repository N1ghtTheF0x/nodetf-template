{
    "name": "@nodetf/<name>",
    "type": "module",
    "version": "1.0.0",
    "description": "",
    "exports": {
      ".": {
        "import": {
          "types": "./esm/index.d.ts",
          "default": "./esm/index.js"
        },
        "require": {
          "types": "./cjs/index.d.ts",
          "default": "./cjs/index.js"
        }
      }
    },
    "files": ["cjs/**/*","esm/**/*"],
    "scripts": {
      "test:cjs": "ts-node --project ./tsconfig.cjs.json test.cts",
      "test:esm": "ts-node --project ./tsconfig.esm.json test.mts",
      "prebuild": "npm run clean",
      "build:cjs": "tsc --project ./tsconfig.cjs.json",
      "build:esm": "tsc --project ./tsconfig.esm.json",
      "build": "npm run build:cjs && npm run build:esm",
      "clean": "rm -rf ./cjs && rm -rf ./esm",
      "prepublishOnly": "npm run build"
    },
    "author": "N1ghtTheF0x",
    "license": "MIT",
    "devDependencies": {
      "ts-node": "latest",
      "typescript": "latest"
    },
    "repository": {
      "type": "git",
      "url": "git+https://github.com/N1ghtTheF0x/nodetf-"
    },
    "types": "./cjs/index.d.ts",
    "main": "./cjs/index.js"
  }
